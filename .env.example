# VentureBots Configuration Template
# Copy this file to .env and fill in your actual values

# Required: Anthropic API key for Claude integration
ANTHROPIC_API_KEY=sk-ant-api03-your-key-here

# Optional: OpenAI API key (if using OpenAI models)
OPENAI_API_KEY=sk-your-openai-key-here

# Optional: SerpAPI key (if using external search)
SERPAPI_API_KEY=your-serpapi-key-here


# database config
SUPABASE_URL=url-here
SUPABASE_ANON_KEY=anon-key-here
SUPABASE_JWKS_URL=jwks-url-here
DATABASE_URL=db-url



# Server Configuration
PORT=8000
HOST=0.0.0.0


# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=data/logs/venturebot.log

# Development Settings
DEBUG=false
DEVELOPMENT=false

# Frontend Configuration
ADK_BACKEND_URL=http://localhost:8000
CHAINLIT_PORT=8501