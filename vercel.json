{
  "version": 2,
  "builds": [
    {
      "src": "chainlit_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/chainlit_app.py"
    }
  ],
  "env": {
    "ADK_BACKEND_URL": "@adk_backend_url"
  },
  "functions": {
    "chainlit_app.py": {
      "runtime": "python3.9"
    }
  }
}